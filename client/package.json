{
  "name": "client",
  "version": "1.0.0",
  "scripts": {
    "start": "npm start",
    "dev": "parcel index.html",
    "build": "parcel build index.html",
    "test": "jest",
    "clean": "rimraf dist .parcel-cache",
    "verify": "npm run test:coverage && tsc",
    "test:coverage": "jest --collectCoverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@babel/core": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@types/jest": "^27.4.0",
    "@types/pretty": "^2.0.1",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "babel": "^6.23.0",
    "babel-jest": "^27.5.0",
    "eslint": "^8.8.0",
    "eslint-plugin-react": "^7.28.0",
    "jest": "^27.5.0",
    "parcel": "^2.2.1",
    "pretty": "^2.0.0",
    "regenerator-runtime": "^0.13.9",
    "rimraf": "^3.0.2",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "follow-redirects": "^1.14.8",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.1"
  },
  "babel": {
    "presets": [
      "@babel/preset-react",
      "@babel/preset-typescript",
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  },
  "jest": {
    "testEnvironment": "jsdom",
    "collectCoverageFrom": [
      "**/*.{jsx,tsx,ts,js}",
      "!.eslintrc.js",
      "!coverage/**",
      "!dist/**",
      "!babel.config.js",
      "!index.tsx",
      "!config/**"
    ]
  }
}
