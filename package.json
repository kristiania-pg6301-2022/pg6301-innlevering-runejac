{
  "name": "innlevering-quiz-med-server-heroku",
  "version": "1.0.0",
  "description": "* [x] lag client siden\r * [x] lag tester for client\r * [ ] coveralls io\r * [ ] lag server siden\r * [ ] lag tester for server\r * [ ] deploy til Heroku",
  "scripts": {
    "start": "npm ci && npm run start:server",
    "start:server": "cd server && npm run start",
    "dev": "concurrently npm:dev:server npm:dev:client",
    "dev:client": "cd client && npm run dev",
    "dev:server": "cd server && npm run dev",
    "build": "npm run build:server && npm run build:client",
    "build:client": "cd client && npm ci --include=dev && npm run build",
    "build:server": "cd server && npm ci",
    "test": "jest",
    "format": "prettier --write **/*.{ts,jsx,js,tsx,json,html}",
    "format:check": "prettier --check **/*.{js,ts,tsx,jsx,json,html}",
    "clean": "cd client && rimraf dist .parcel-cache",
    "verify": "npm run test:coverage && npm run verify:server && npm run verify:client",
    "verify:server": "cd server && npm ci && npm run verify",
    "verify:client": "cd client && npm ci && npm run verify",
    "prepare": "husky install",
    "test:coverage": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/runejac/web-api-own-account-innlevering-runejac"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/runejac/web-api-own-account-innlevering-runejac/issues"
  },
  "homepage": "https://github.com/runejac/web-api-own-account-innlevering-runejac#readme",
  "devDependencies": {
    "@babel/core": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-flow": "^7.16.7",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@types/body-parser": "^1.19.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/react-router-dom": "^5.3.3",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "babel": "^6.23.0",
    "babel-jest": "^27.5.0",
    "concurrently": "^7.0.0",
    "eslint": "^8.8.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-react": "^7.28.0",
    "husky": "^7.0.4",
    "jest": "^27.5.0",
    "parcel": "^2.2.1",
    "prettier": "^2.5.1",
    "supertest": "^6.2.2",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "body-parser": "^1.19.1",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.0",
    "express": "^4.17.2",
    "follow-redirects": "^1.14.8",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.1",
    "ts-node": "^10.5.0"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      "@babel/preset-typescript",
      "@babel/preset-flow"
    ]
  },
  "jest": {
    "testEnvironment": "jsdom",
    "collectCoverageFrom": [
      "**/*.{jsx,tsx,ts,js}",
      "!.eslintrc.js",
      "!coverage/**",
      "!client/coverage/**",
      "!client/dist/**",
      "!server/server.ts",
      "!dist/**",
      "!babel.config.js",
      "!client/index.tsx",
      "!config/**"
    ]
  },
  "engines": {
    "node": "16.13.1",
    "npm": "8.4.1"
  }
}
