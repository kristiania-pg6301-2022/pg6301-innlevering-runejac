{
  "name": "innlevering-quiz-med-server-heroku",
  "version": "1.0.0",
  "description": "* [x] lag client siden\r * [x] lag tester for client\r * [ ] coveralls io\r * [ ] lag server siden\r * [ ] lag tester for server\r * [ ] deploy til Heroku",
  "main": "index.js",
  "scripts": {
    "start": "npm start",
    "dev": "cd client && parcel index.html",
    "test": "jest",
    "clean": "rimraf dist .parcel-cache",
    "format": "prettier --write *.{ts,jsx,js,tsx} __tests__/**/*.{ts,jsx,js,tsx}",
    "format:check": "prettier --check *.{ts,jsx,js,tsx} __tests__/**/*.{ts,jsx,js,tsx}",
    "lint": "eslint **/*.{ts,tsx,js,jsx}",
    "test:coverage": "jest --coverage",
    "verify": "cd client && npm run format:check && tsc && npm run lint && npm run test:coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/runejac/innlevering-quiz-med-server-heroku.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/runejac/innlevering-quiz-med-server-heroku/issues"
  },
  "homepage": "https://github.com/runejac/innlevering-quiz-med-server-heroku#readme",
  "devDependencies": {
    "@babel/core": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@types/jest": "^27.4.0",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "babel": "^6.23.0",
    "babel-jest": "^27.5.0",
    "eslint": "^8.8.0",
    "eslint-plugin-react": "^7.28.0",
    "jest": "^27.5.0",
    "parcel": "^2.2.1",
    "prettier": "^2.5.1",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.1"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react",
      "@babel/preset-typescript"
    ]
  },
  "jest": {
    "testEnvironment": "jsdom",
    "collectCoverageFrom": [
      "**/*.{jsx,tsx,ts,js}",
      "!.eslintrc.js",
      "!coverage/**",
      "!dist/**",
      "!babel.config.js",
      "!index.tsx",
      "!config/**"
    ]
  }
